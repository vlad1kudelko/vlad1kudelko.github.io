mixin _breadcrumb(inp_arr)
    nav(class='flex')
      ol(class='inline-flex items-center space-x-1 md:space-x-2 rtl:space-x-reverse')
        li(class='inline-flex items-center')
          a(class='inline-flex items-center text-sm font-medium text-gray-400 hover:text-white' href='/')
            svg.w-3.h-3(class='me-2.5' fill='currentColor' viewbox='0 0 20 20')
              path(d='m19.707 9.293-2-2-7-7a1 1 0 0 0-1.414 0l-7 7-2 2a1 1 0 0 0 1.414 1.414L2 10.414V18a2 2 0 0 0 2 2h3a1 1 0 0 0 1-1v-4a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1v4a1 1 0 0 0 1 1h3a2 2 0 0 0 2-2v-7.586l.293.293a1 1 0 0 0 1.414-1.414Z') Home
        each val in inp_arr
            li
              div(class='flex items-center')
                svg.w-3.h-3.text-gray-400.mx-1(class='rtl:rotate-180' fill='none' viewbox='0 0 6 10')
                  path(stroke='currentColor' stroke-linecap='round' stroke-linejoin='round' stroke-width='2' d='m1 9 4-4-4-4')
                if val.url
                    a(class='ms-1 text-sm font-medium md:ms-2 text-gray-400 hover:text-white' href=val.url)= val.text
                else
                    span(class='ms-1 text-sm font-medium md:ms-2 text-gray-400')= val.text
